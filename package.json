{
	"name": "worker-workflow-test",
	"module": "src/index.ts",
	"type": "module",
	"packageManager": "bun@1.1.42",
	"scripts": {
		"lint": "eslint \"{src,test,scripts}/**/*.{ts,mjs}\"",
		"lint-fix": "bun lint --fix",
		"test": "vitest",
		"typecheck": "tsc -p tsconfig.src.json --noEmit",
		"madge": "madge --ts-config ./tsconfig.src.json --circular --no-color --no-spinner --extensions ts,tsx src",
		"deploy": "wrangler deploy --name=$(jq -r .name package.json)",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"cf-typegen": "wrangler types"
	},
	"browserslist": {
		"production": ["defaults", "not op_mini all"],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@effect/experimental": "^0.46.0",
		"@effect/platform": "^0.82.0",
		"@effect/sql": "^0.35.0",
		"@effect/sql-d1": "^0.33.0",
		"effect": "^3.15.0"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.27",
		"@cloudflare/workers-types": "^4.20250510.0",
		"@effect/eslint-plugin": "^0.3.2",
		"@effect/language-service": "^0.12.1",
		"@eslint/compat": "1.2.9",
		"@eslint/eslintrc": "3.3.1",
		"@eslint/js": "9.26.0",
		"@eslint/json": "0.12.0",
		"@eslint/markdown": "6.4.0",
		"@types/bun": "^1.2.13",
		"@typescript-eslint/eslint-plugin": "^8.32.0",
		"@typescript-eslint/parser": "^8.32.0",
		"eslint": "^9.26.0",
		"eslint-import-resolver-typescript": "^4.3.4",
		"eslint-plugin-deprecation": "^3.0.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-sort-destructure-keys": "^2.0.0",
		"lefthook": "^1.11.12",
		"madge": "^8.0.0",
		"prettier": "^3.5.3",
		"typescript": "^5.8.3",
		"vitest": "^3.1.3",
		"wrangler": "^4.14.4"
	}
